{
  "name": "movie-time",
  "version": "1.0.0",
  "description": "Qantas Developer Challenge",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --watch",
    "lint": "eslint source/**/*.js",
    "dev": "watchify -p [ css-modulesify -o http/assets/main.css ] source/main.js -o http/assets/main.js -v",
    "serve": "browser-sync start -s 'http' -f 'http' --no-online",
    "start": "browserify source/main.js -o http/assets/main.js"
  },
  "author": "Michael Hazell",
  "license": "ISC",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ],
    "plugins": [
      "css-modulesify"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "css-modulesify": "^0.25.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "watchify": "^3.7.0"
  }
}
