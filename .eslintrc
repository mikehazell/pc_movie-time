{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "indent": [2, 4],
      "arrow-body-style": [0],
      "react/jsx-indent": [2, 4],
      "react/jsx-indent-props": [2, 4],
      "react/prefer-stateless-function": [0],
      "no-restricted-syntax": [2, 'DebuggerStatement'],
      "no-console": [0]
    }
}
